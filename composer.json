{
  "name": "p3in/app-compass",
  "description": "Plus 3 Interactive's App Management System",
  "authors": [
    {
      "name": "Plus 3 Interactive, LLC",
      "email": "support@p3in.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "laravel/framework": "5.5.*",
    "laravel/passport": "^3.0",
    "tymon/jwt-auth": "dev-develop",
    "pusher/pusher-php-server": "~3.0",
    "guzzlehttp/guzzle": "~6.0",
    "p3in/modular": "dev-develop",
    "p3in/form-builder": "dev-develop"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "P3in\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "P3in\\Providers\\ModuleServiceProvider",
        "P3in\\Providers\\FormBuilderServiceProvider",
        "P3in\\Providers\\AppCompassServiceProvider",
        "P3in\\Providers\\AppCompassEventServiceProvider"
      ]
    }
  }
}
