{
  "name": "appcompass/appcompass",
  "description": "App Compass - An App Management System",
  "authors": [
    {
      "name": "Jubair Saidi",
      "email": "jubair.saidi@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "fideloper/proxy": "~4.0",
    "laravel/tinker": "~1.0",
    "laravel/framework": "5.6.*",
    "laravel/passport": "~5.0",
    "predis/predis": "^1.1",
    "optimus/bruno": "^3.1",
    "tymon/jwt-auth": "dev-develop",
    "propaganistas/laravel-phone": "^3.0",
    "pusher/pusher-php-server": "~3.0",
    "guzzlehttp/guzzle": "~6.0",
    "appcompass/skeleton-mapper": "dev-develop"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "AppCompass\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AppCompass\\Providers\\AppCompassServiceProvider"
      ]
    }
  }
}
